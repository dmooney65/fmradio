{
  "name": "fmradio",
  "description": "FM Radio app",
  "version": "0.1.2",
  "main": "app/main.js",
  "scripts": {
    "start": "electron-forge start",
    "rebuild": "electron-rebuild --version 1.7.5 -f -w fmradio",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "electron-forge lint"
  },
  "author": "Dominic Mooney",
  "keywords": [
    "sdr",
    "electron"
  ],
  "style": "css/photon.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/dmooney65/fmradio.git"
  },
  "bugs": {
    "url": "https://github.com/dmooney65/fmradio/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^4.0.0",
    "electron-forge-maker-nsis": "^19.34.0",
    "electron-installer-flatpak": "^0.6.0",
    "electron-prebuilt-compile": "1.7.5",
    "electron-rebuild": "1.6.0",
    "eslint": "^4.7.2"
  },
  "dependencies": {
    "electron-compile": "^6.4.1",
    "fft-stream": "0.0.0",
    "node-flac": "dmooney65/node-flac",
    "node-ssdp": "^3.2.0",
    "node-upnp-utils": "0.0.4",
    "sdrjs": "dmooney65/sdrjs",
    "to-arraybuffer": "^1.0.1",
    "upnp-mediarenderer-client": "^1.2.4",
    "wav": "^1.0.1",
    "xml2js": "^0.4.17"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel",
          "nsis"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "ignore": [
          "build_scripts",
          "*.js"
        ],
        "afterCopy": [
          "./build_scripts/afterCopy.js"
        ],
        "afterExtract": [
          "./build_scripts/afterExtract.js"
        ]
      },
      "electronWinstallerConfig": {},
      "electronInstallerDebian": {
        "icon": "icons/tower.png"
      },
      "electronInstallerRedhat": {
        "icon": "icons/tower.png"
      },
      "electronInstallerFlatpak": {
        "icon": "icons/tower.png"
      },
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "fmradio"
      }
    }
  }
}